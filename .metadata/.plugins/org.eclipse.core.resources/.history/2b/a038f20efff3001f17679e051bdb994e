package allclass;

import javax.persistence.Entity;


public class LineOrderItem {
	
	private long id;
//	private int item_id;
	private int qty;
//	private long order_id;
	
	
	public LineOrderItem() {};
	
	
	
	
	public LineOrderItem(long id, int qty) {
		super();
		this.id = id;
		
		this.qty = qty;
		
	}

	@Override
	public String toString() {
		return "OrderItem [id=" + id  + ", qty=" + qty + ", order_id=]";
	}




	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	
	
	
	public int getQty() {
		return qty;
	}
	public void setQty(int qty) {
		this.qty = qty;
	}
	
	
	

}

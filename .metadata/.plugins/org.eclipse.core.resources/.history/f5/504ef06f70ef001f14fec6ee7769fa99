package Hibernate;

import java.io.Serializable;

public class StudentId implements Serializable{

	private int rollno;
	private int year;
	
	public StudentId() {}
	
	public StudentId(int rollno, int year) {
		super();
		this.rollno = rollno;
		this.year = year;
	}
	public int getRollno() {
		return rollno;
	}
	public void setRollno(int rollno) {
		this.rollno = rollno;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
}
